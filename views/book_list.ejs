<%- include('../views/partials/_head.ejs') %> 
<body>
        <%- include('../views/partials/navbar.ejs') %> 
      <div class="col-sm-10">
        <h1><%= title %> </h1>
        <ul>
            <% book_list.sort(function(a, b) {let textA = a.title.toUpperCase(); let textB = b.title.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;}); book_list.forEach((book) => {%> 
                <li>
                    <a href="<%= book.url %> "><%= book.title %> </a> (<%= book.author.name%>)
                </li>
            <% }) %> 
        </ul>
      </div>
    </div>
  </div>
</body>
</html>
