<%- include('../views/partials/_head.ejs') %> 
<body>
        <%- include('../views/partials/navbar.ejs') %> 
      <div class="col-sm-10">
        <h1>Genre: <%= genre.name %> </h1>
        <div>
            <h4>Books</h4>
            <dl>
                <% genre_books.forEach((book) => { %>
                    <dt>
                        <a href="<%= book.url%> "> <%= book.title %> </a>
                    </dt>
                    <dd>
                        <%= book.summary %> 
                    </dd>

                <% }) %> 
            </dl>
        </div>
        <hr>
        <p><a href="<%= genre.url + '/delete' %> ">delete</a></p>
        <p><a href="<%= genre.url + '/update' %> ">Update</a></p>
      </div>
      
    </div>
  </div>
</body>
</html>